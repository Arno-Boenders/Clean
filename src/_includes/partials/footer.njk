{% import "components/navigation.njk" as nav with context %}
<nav class="nav">
    <a href="/" class="nav__link">Clean</a>
    {{nav.nav()}}

    <ul class="nav__list">
        {% for item in collections.policyItems | sortByOrder %}
            <li class="nav__list__item">
                <a href="{{item.url}}" class="nav__link">{{item.data.pageTitle}}</a>
            </li>
        {% endfor %}
    </ul>
</nav>